{
	"type": "minecraft:simple_input_form",
	"title": [
		"Generator: ",
		{
			"text": "replacement_block",
			"color": "green"
		}
	],
	"external_title": {
		"text": "replacement_block",
		"color": "green"
	},
	"can_close_with_escape": true,
	"body": [
		{
			"type": "minecraft:plain_message",
			"contents": "Give yourself a block-placing item",
			"width": 400
		}
	],
	"inputs": [
		{
			"type": "minecraft:text",
			"key": "block",
			"initial": "pink_petals[flower_amount=4]",
			"label": "Enter the block state here:",
			"label_visible": true
		}
	],
	"action": {
		"label": ">> Generate Item <<",
		"id": "test_submission",
		"on_submit": {
			"type": "minecraft:command_template",
			"template": "function pandabuild:give/replacement_block {block:\"$(block)\"}"
		}
	}
}
